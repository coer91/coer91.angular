{
  "name": "coer91.angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "server": "ng serve documentation",
    "update:styles": "npm uninstall coer91.styles & npm cache clean --force & npm install coer91.styles@latest",
    "update:tools": "npm uninstall coer91.tools & npm cache clean --force & npm install coer91.tools@latest",
    "build:styles": "sass --no-source-map --style=compressed projects/coer91.angular/index.scss ./dist/coer91.angular/styles.scss",
    "build:lib": "ng build coer91.angular & npm run build:styles"
  },
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "coer91.styles": "^0.0.40",
    "coer91.tools": "^0.0.68",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}